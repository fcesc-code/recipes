{
  "name": "fcesc-boilerplate",
  "version": "0.0.1",
  "description": "Boilerplate for vanillaJS projects",
  "main": "src/index.html",
  "scripts": {
    "parcel:dev": "parcel src/index.html -p 9200 --open",
    "parcel:build": "parcel build src/index.html --public-url ./ --out-dir dist",
    "clean": "rimraf dist .cache",
    "dev": "npm-run-all clean parcel:dev",
    "devserver": "npm-run-all clean babel:dev",
    "babel:dev": "set DEBUG=server & babel-node src/server.js --exec",
    "build": "npm-run-all clean parcel:build",
    "lint": "./node_modules/.bin/eslint src --fix",
    "twatch": "jest --watchAll",
    "tcoverage": "jest --coverage",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/node": "^7.13.13",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.14.1",
    "autoprefixer": "^9.8.6",
    "ejs": "^3.1.6",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.14",
    "prettier": "^2.2.1",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^3.0.2",
    "sass": "^1.32.12"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "debug": "^4.3.1",
    "express": "^4.17.1",
    "parcel-bundler": "^1.12.5"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "env": {
      "test": {
        "plugins": [
          "@babel/plugin-transform-runtime"
        ]
      }
    }
  },
  "browserslist": [
    "last 5 versions or > 2%",
    "not dead",
    "IE 11"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fcesc-code/uoc-eines1-pac1.git"
  },
  "keywords": [
    "Vanilla",
    "JS",
    "UOC"
  ],
  "author": {
    "name": "Francesc Brugarolas"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fcesc-code/uoc-eines1-pac1/issues"
  },
  "homepage": "https://github.com/fcesc-code/uoc-eines1-pac1#readme"
}
